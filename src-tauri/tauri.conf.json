{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "JDfund",
  "version": "2.0.0",
  "identifier": "com.jdf.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "width": 280,
        "height": 40,
        "minWidth": 100,
        "minHeight": 30,
        "maxWidth": 400,
        "maxHeight": 60,
        "transparent": true,
        "alwaysOnTop": true,
        "hiddenTitle": true,
        "decorations": false,
        "skipTaskbar": true,
        "center": false,
        "x": -1,
        "y": -1
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
